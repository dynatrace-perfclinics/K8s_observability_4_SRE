<scenario postMonitoringTime="-1" preMonitoringTime="-1"
    slaProfileEnabled="false" traceVariables="true" uid="scenario1" virtualUsersStates="true">
    <!--****** POPULATION POLICY ******-->
    <population-policy name="buyer">
        <duration-policy-entry iterations="1" time="2100" timeUnit="0" type="2"/>
        <volume-policy-entry>
            <rampup-volume-policy delayIncrement="1.0"
                delayTypeIncrement="2" initialUserNumber="20"
                iterationNumber="1" maxUserNumber="100" userIncrement="30"/>
        </volume-policy-entry>
        <start-stop-policy-entry start-delay="0" start-type="0"
            stop-delay="60000" stop-type="0"/>
        <runtime-policy continueOnError="true" thinktimePolicy="0"
            thinktimeValue="5000" vuStartDelay="30000" vuStartMode="1"/>
        <!--****** POPULATION LG HOSTS ******-->
        <lg-hosts>
            <lg-host-entry>$zoneID=Default zone;$lgID=localhost:7100</lg-host-entry>
        </lg-hosts>
    </population-policy>
    <!--****** DASHBOARDS  ******-->
    <!--****** END DASHBOARDS  ******-->
    <!--****** START DATA FILTERS  ******-->
    <!--****** END DATA FILTERS  ******-->
    <!--****** START RENDEZ-VOUS POLICIES  ******-->
    <!--****** END RENDEZ-VOUS POLICIES  ******-->
    <!--****** START DEBUG POLICY ******-->
    <debug-policy mode="0"/>
    <!--****** END DEBUG POLICY ******-->
    <!--****** START VU STATES ******-->
    <!--****** END VU STATES ******-->
    <!--****** START EXCLUSION FILTER ******-->
    <request-path-exclusion-filter isEnabled="false">
        <regexps>
            <regexp>.*\.jpg</regexp>
            <regexp>.*\.jpeg</regexp>
            <regexp>.*\.gif</regexp>
            <regexp>.*\.png</regexp>
            <regexp>.*\.bmp</regexp>
            <regexp>.*\.css</regexp>
            <regexp>.*\.js</regexp>
            <regexp>.*\.swf</regexp>
            <regexp>.*\.ico</regexp>
            <regexp>.*\.jar</regexp>
            <regexp>.*\.class</regexp>
            <regexp>.*\.properties</regexp>
        </regexps>
    </request-path-exclusion-filter>
    <!--****** END  EXCLUSION FILTER ******-->
    <!--****** START DYNATRACE MONITORING ******-->
    <dynatrace-monitoring/>
    <!--****** END  DYNATRACE MONITORING ******-->
    <!--****** START TRACE VARIABLES ******-->
    <!--****** END TRACE VARIABLES ******-->
</scenario>
